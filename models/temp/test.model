continuous reachability
{
  state var x1,x2

  setting
  {
    fixed steps 0.1
    time 1.0
    remainder estimation 1e-3
    shrink wrapping 0
    gnuplot interval x1,x2
    fixed orders 4
    cutoff 1e-15
    precision 53
    output test
    alg_small_comp
    decomposition [[x1,x2]]
    print on
  }

  poly ode 1
  {
    x1' = -x1
    x2' = -x2 * x1
  }

  init
  {
    x1 in [-2,4]
    x2 in [-1,1]
  }
}
