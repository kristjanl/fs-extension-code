continuous reachability
{
  state var x1,x2,x3,x4,x5

  setting
  {
    fixed steps 0.1
    time 10.0
    remainder estimation 1e-3
    shrink wrapping 0
    gnuplot interval x1,x2
    fixed orders 2
    cutoff 1e-15
    precision 53
    output test
    alg_small_comp
    decomposition [[x1,x2,x4],[x3],[x5]]
    print on
  }

  poly ode 1
  {
    x1' = 1
    x2' = 2
    x3' = 3
    x4' = 4
    x5' = x1
  }

  init
  {
    x1 in [-1,0]
    x2 in [0,0]
    x3 in [0,0]
    x4 in [0,0]
    x5 in [0,0]
  }
}
