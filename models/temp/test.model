continuous reachability
{
  state var x1

  setting
  {
    fixed steps 0.1
    time 1.0
    remainder estimation 1e-3
    QR precondition
    gnuplot interval x1,x1
    fixed orders 2
    cutoff 1e-15
    precision 53
    output test
    alg_small_comp flow impl
    decomposition [[x1]]
    print on
  }

  poly ode 1
  {
    x1' = -x1
  }

  init
  {
    x1 in [-1,1]
  }
}
