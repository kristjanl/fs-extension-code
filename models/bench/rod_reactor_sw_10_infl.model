continuous reachability
{
 state var x,c1,c2
	
 setting
 {
  fixed steps 0.1
  time 50
  remainder estimation 1e-5
  shrink wrapping 10
  gnuplot octagon x,c1
  fixed orders 5
  cutoff 1e-15
  precision 128
  output rod_reactor_sw_10_infl
  alg_small_comp
  no decomposition
  print on
 }
	
 poly ode 1
 {
    x' = 0.1*x - 50
    c1' = 1
    c2' = 1
 }
 
 init
 {
   x in [510,520]
   c1 in [19.99999,20.00001]
   c2 in [19.99999,20.00001]
 }
}

