continuous reachability
{
 state var x, y, z
 setting
 {
  fixed steps 0.02
  time 10
  remainder estimation 1e-4
  shrink wrapping 0
  gnuplot octagon x,y
  fixed orders 5
  cutoff 1e-12
  precision 53
  output wattSteam_fcomp
  alg_small_comp flow impl
  no decomposition
  print on
 }
	
 poly ode 1
 {
  x' = y
  y' = (z^2*cos(x) - 1)*sin(x) - 3*y
  z' = cos(x) - 1
 }
 
 init
 {
  x in [0.9,1.1]
  y in [-0.1,0.1]
  z in [0.9,1.1]
 }
}
