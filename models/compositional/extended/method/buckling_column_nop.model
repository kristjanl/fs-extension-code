continuous reachability
{
  state var x,y

  setting
  {
    use cflow
    no components
    fixed steps 0.01
    time 10
    remainder estimation 1e-6
    no processing
    gnuplot octagon x,y
    fixed orders 4
    cutoff 1e-12
    precision 53
    output buckling_column_nop
    print on
  }

  poly ode 1
  {
    x' = y
    y' = 2 * x - x^3 - 0.2 * y + 0.1
  }

  init
  {
    x in [-0.5 , -0.4]
    y in [-0.5 , -0.4]
  }
}
