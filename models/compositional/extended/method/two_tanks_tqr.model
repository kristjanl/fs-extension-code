continuous reachability
{
  state var x1,x2
    
  setting
  {
    use cflow
    no components
    fixed steps 0.01
    time 2
    remainder estimation 1e-3
    TQR precondition
    gnuplot octagon x1,x2
    fixed orders 10
    cutoff 1e-15
    precision 128
    output two_tanks_tqr
    print on
  }
    
  poly ode 1
  {
    x1' = -x1 - 2 + [-0.01,0.01]
    x2' = x1 - x2 - 5 + [-0.01,0.01]
  }
  
  init
  {
    x1 in [1.5,2.5]
    x2 in [1,1]
  }
}

