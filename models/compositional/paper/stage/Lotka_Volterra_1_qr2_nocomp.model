continuous reachability
{
 state var x0,y0
	
 setting
 {
  fixed steps 0.02
  time 10
  remainder estimation 1e-3
  QR precondition2
  gnuplot octagon x0,x0
  fixed orders 4
  cutoff 1e-20
  precision 53
  output Lotka_Volterra_1_qr2_nocomp
  alg_small_comp flow impl
  no decomposition
  print on
 }
	
 poly ode 1
 {
  x0' = 1.5*x0 - x0*y0
  y0' = -3*y0 + x0*y0
 }
 
 init
 {
  x0 in [4.8 , 5.2]
  y0 in [1.8 , 2.2]
 }
}

