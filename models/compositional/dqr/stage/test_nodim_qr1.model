continuous reachability
{
  state var x1,x2,x3

  setting
  {
    fixed steps 0.1
    time 2
    remainder estimation 1e-3
    QR precondition1
    gnuplot interval x1,x1
    fixed orders 5
    cutoff 1e-15
    precision 53
    output testnodim_qr1
    alg_small_comp flow impl
    no decomposition
    print on
  }

  poly ode 1
  {
    x1' = x1
    x2' = x2
    x3' = x1 + x3
  }

  init
  {
    x1 in [-0.5,0.5]
    x2 in [-0.4,0.4]
    x3 in [-0.25,0.25]
  }
}
