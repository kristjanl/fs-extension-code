continuous reachability
{
  state var x1,x2,x3

  setting
  {
    fixed steps 0.1
    time 10
    remainder estimation 1e-3
    QR precondition
    gnuplot interval x1,x1
    fixed orders 5
    cutoff 1e-15
    precision 53
    output and_linnodim_qrplain
    alg_small_comp flow impl
    no decomposition
    print on
  }

  poly ode 1
  {
    x1' = -0.3*x1 + 0.1*x2
    x2' = 0.3*x1 - (0.1 + 0.9)*x2
    x3' = 0.9*x2
  }

  init
  {
    x1 in [0.99,1.01]
    x2 in [0,0]
    x3 in [0,0]
  }
}
