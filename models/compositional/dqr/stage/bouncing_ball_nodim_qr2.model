continuous reachability
{
 state var x,v
	
 setting
 {
  fixed steps 0.1
  time 10
  remainder estimation 1e-5
  QR precondition2
  gnuplot octagon x,v
  fixed orders 5
  cutoff 1e-15
  precision 128
  output bouncing_ballnodim_qr2
  alg_small_comp flow impl
  no decomposition
  print on
 }
	
 poly ode 1
 {
    x' = v
    v' = -9.81
 }
 
 init
 {
   x in [10,10.2]
   v in [0,0]
 }
}

