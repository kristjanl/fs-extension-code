continuous reachability
{
  state var x1,x2,x3

  setting
  {
    fixed steps 1
    time 100
    remainder estimation 1e-3
    no precondition
    gnuplot interval x1,x1
    fixed orders 5
    cutoff 1e-15
    precision 53
    output sq_deg_long_3_nocomp
    alg_small_comp flow impl
    no decomposition
    print on
  }

  poly ode 1
  {
    x1' = -0.1*x1*x1
    x2' = -0.1*x2*x2
    x3' = -0.1*x3*x3
  }

  init
  {
    x1 in [0.5,1.0]
    x2 in [0.5,1.0]
    x3 in [0.5,1.0]
  }
}
